      "value": "$piezoelectricSensorLocation"
    },
    "afmChannel": {
      "height": {
        "value": "$height",
        "description": "Height image from AFM instrument."
      },
      "amplitude": {
        "value": "$amplitude",
        "description": "Amplitude image from AFM instrument."
      },
      "phase": {
        "value": "$phase",
        "description": "Phase image from AFM instrument."
      },
      "zSensor": {
        "value": "$zSensor",
        "description": "Z-sensor image from AFM instrument."
      }
