  "experiment": {
    "value": "$experiment",
    "sampleID": {
      "description": "Sample ID of the AFM instrument.",
      "value": "$sampleID"
    },
    "scanParameters": {
      "scanSize": {
        "value": "$scanSize",
        "description": "Size of the AFM scan along one side of the square."
      },
      "scanRate": {
        "value": "$scanRate",
        "description": "Controls the rate at which the cantilever scans the sample area."
      },
      "scanAngle": {
        "value": "$scanAngle",
        "description": "Angle at which the scanning occurs."
      },
      "scanLines": {
        "value": "$scanLines",
        "description": "Number of lines scanned per second in the fast scan direction (X-axis)."
      }
    },
    "afmVoltage": {
      "description": "Voltage of the AFM experiment.",
      "value": "$afmVoltage"
    },
    "springConstant": {
      "description": "Spring constant associated with AFM experiment.",
      "value": "$springConstant"
    },
    "gainParameters": {
      "integralGain": {
        "value": "$integralGain",
        "description": "Controls response time of the feedback loop; can dramatically affect the image quality and dependent on both the duration and magnitude of the error signal."
      },
      "proportionalGain": {
        "value": "$proportionalGain",
        "description": "Scales the error signal by a user specified value."
      }
    },
    "setPoint": {
      "description": "Deflection or amplitude that the height feedback maintains during imaging.",
      "value": "$setPoint"
    },
    "driveAmplitude": {
      "description": "Amplitude of the voltage to cause the cantilever to vibrate.",
      "value": "$driveAmplitude"
    },
    "resonanceFrequency": {
      "description": "Resonance frequency of the cantilever.",
