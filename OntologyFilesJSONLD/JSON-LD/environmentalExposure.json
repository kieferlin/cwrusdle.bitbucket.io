    "environmentalExposure": 
    {
      "description": "Exposure to environmental stressors result in different degradation processes",
      "degradation": 
      {
        "description": "Degradation results in performnace loss",
		      	"thermalDegradation": 
		      	{
		      	  "description": "Thermal degradation takes place when the polymer changes its properties due to increased temperature",
                 "heat" : {
                "temperature" : {
                  "description": "Temperature of the environment",
                  "value": "$temperature",
                  "unitText": "Celsius",
                  "@type": "float"
                }
                }
		      	  
		      	},
                            
		      	"hydrolyticDegradation": {
		      	  "description": "Hydrolytic degradation takes place when the polymer bonds react with water molecules and modify the chains",
                  "moisture" : {
                  "waterAbsorption" : {
                  "description": "Transport of water",
                  "value": "$waterAbsorption",
                  "@type": "string",
                  "relativeHumidity" : {
                  "description": "The percent ratio of the amount of water vapor in the air at a given temperature relative the amount of the maximum possible water vapor",
                  "value": "$relativeHumidity",
                  "unitText": "%",
                  "@type": "float"
                },
                "absoluteHumidity" : {
                  "description": "The actual amount of water vapor in the air",
                  "value": "$absoluteHumidity",
                  "unitText": "g/m3",
                  "@type": "float"
                }
                }
                }                       
                },
                 
		      	"photodegradation": {
		      	  "description": "Photodegradation is caused by the absorption of photons",
		      	  "radiation" : {
                "uvLight" : {
                  "description": "UV light radiation",
                  "value": "$uvLight",
                  "unitText": "mW/cm2",
                  "@type": "float"
                }
		      	  }
                },
                "synergyEffect": {
                  "description": "Different degradation processes happen simultaneouly",
                   "value": "$synergyEffect",
                   "@type": "string"
                }
                }
                }
                }

                 
		      	
	
