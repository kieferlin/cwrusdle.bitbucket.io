    skos:altLabel "" ;
    skos:definition "Method 1 to perform dark correction." .

mds-XRDPostProcessing:DarkCorrectionMethod2 a owl:Class ;
    rdfs:label "DarkCorrectionMethod2" ;
    rdfs:subClassOf mds-XRDPostProcessing:DarkCorrection ;
    skos:altLabel "" ;
    skos:definition "Method 2 to perform dark correction." .

mds-XRDPostProcessing:DistanceBetweenTwoRings a owl:Class ;
    rdfs:label "DistanceBetweenTwoRings" ;
    rdfs:subClassOf pmd:Distance ;
    skos:altLabel "" ;
