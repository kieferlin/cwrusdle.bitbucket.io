pmd:characteristicOf a owl:ObjectProperty ;
    rdfs:label "characteristic of"@en ;
    rdfs:domain mds-pv:PhotovoltaicBacksheet,
        mds-pv:PhotovoltaicCell,
        mds-pv:PhotovoltaicInverter,
        mds-pv:PhotovoltaicModule,
        mds-pv:PhotovoltaicRacking,
        mds-pv:PhotovoltaicSite ;
    rdfs:range mds-pv:Length,
        mds-pv:MountingType,
        mds-pv:PhotovoltaicBacksheetProperty,
        mds-pv:PhotovoltaicCellProperty,
        mds-pv:PhotovoltaicInverterProperty,
        mds-pv:PhotovoltaicModuleProperty,
        mds-pv:PhotovoltaicSite,
        mds-pv:PhotovoltaicSiteParameter,
        mds-pv:RackingType,
        mds-pv:Thickness,
        mds-pv:TrackingAlgorithm,
        mds-pv:Width,
        pmd:MaterialDesignation ;
    skos:altLabel "is characteristic of" .

pmd:composedOf a owl:ObjectProperty ;
    rdfs:label "composed of"@en ;
    rdfs:domain mds-pv:PhotovoltaicModule ;
    rdfs:range mds-pv:PhotovoltaicBacksheet,
        mds-pv:PhotovoltaicCell,
        mds-pv:PhotovoltaicInverter ;
    skos:definition """This property is used to describe the fact that and which objects are composed of other objects, i.e. it is used to describe the specific constituents or ingredients that form a particular entity or object. 
As a prepositional phrase, it indicates the elements, parts, or components that make up a whole."""@en,
        "This property is used to describe which voxels and materials an object is composed of."@en .

pmd:hasIdentifier a owl:ObjectProperty ;
    rdfs:domain mds-pv:PhotovoltaicBacksheet,
        mds-pv:PhotovoltaicCell,
        mds-pv:PhotovoltaicInverter,
        mds-pv:PhotovoltaicModule,
        mds-pv:PhotovoltaicSite ;
    rdfs:range mds-pv:PhotovoltaicBacksheetID,
        mds-pv:PhotovoltaicCellID,
        mds-pv:PhotovoltaicInverterID,
        mds-pv:PhotovoltaicModuleID,
        mds-pv:PhotovoltaicSiteID .

pmd:hasSource a owl:ObjectProperty ;
    rdfs:domain mds-pv:InverterEfficiency,
        mds-pv:InverterOutput,
        mds-pv:ModuleRatingAtReference ;
    rdfs:range mds-pv:OrangeButtonTerm .

pmd:value a owl:DatatypeProperty ;
    rdfs:label "hat Wert"@de,
        "value"@en ;
    rdfs:domain mds-pv:BacksheetManufacturer,
        mds-pv:BacksheetMaterial,
        mds-pv:BacksheetVintage,
        mds-pv:CECListingDate,
        mds-pv:CellCount,
        mds-pv:CurrentACMax,
        mds-pv:CurrentAtMaximumPower,
        mds-pv:CurrentShortCircuit,
        mds-pv:EfficiencyCECWtdVoltageMax,
        mds-pv:EfficiencyCECWtdVoltageMin,
        mds-pv:EfficiencyCECWtdVoltageNom,
        mds-pv:Elevation,
        mds-pv:FluorescencePattern,
        mds-pv:FrequenciesAC,
        mds-pv:HasMeter,
        mds-pv:InverterEfficiency,
        mds-pv:InverterEfficiencyCEC,
        mds-pv:InverterEfficiencyIEC,
        mds-pv:InverterEfficiencyMax,
        mds-pv:InverterOutput,
        mds-pv:IsHybrid,
        mds-pv:IsPVInputCapable,
        mds-pv:Latitude,
        mds-pv:Length,
        mds-pv:Longitude,
        mds-pv:ModuleCountSeries,
        mds-pv:ModuleEfficiency,
        mds-pv:ModuleRatingAtReference,
        mds-pv:ModuleRatingCondition,
        mds-pv:MountingType,
        mds-pv:NumberInverters,
        mds-pv:PhotovoltaicBacksheetID,
        mds-pv:PhotovoltaicCellID,
        mds-pv:PhotovoltaicCellProperty,
        mds-pv:PhotovoltaicInverterID,
        mds-pv:PhotovoltaicModuleID,
        mds-pv:PhotovoltaicSiteID,
        mds-pv:PowerACMax,
        mds-pv:PowerACNom,
        mds-pv:PowerDC,
        mds-pv:PowerDCOut,
        mds-pv:PowerSTC,
        mds-pv:RackingType,
        mds-pv:Thickness,
        mds-pv:TrackingAlgorithm,
        mds-pv:VoltageACMax,
        mds-pv:VoltageACMin,
        mds-pv:VoltageAtMaximumPower,
        mds-pv:VoltageOpenCircuit,
        mds-pv:Weight,
        mds-pv:Width,
        pmd:MaterialDesignation ;
    rdfs:range xsd:boolean,
        xsd:float,
        xsd:integer,
        xsd:string ;
    skos:definition "Provides the actual value a value object is referring to."@en .

